<% form_for(@timeslot) do |f| %>
  <%= f.error_messages %>
  <div class="form-row">
    <div class="form-group col-md-6" style="margin-left: 30%;">
      <%= label_tag "Doctor:"%>
      <%= collection_select(:doctor, :id, @doctors, :id, :first_name, {:include_blank => "Select Doctor"})%></br>
    </div>
  </div>
<div class="form-row w-23" style="margin-top: 10px;">
  <div class="form-group col-md-6 datepick">
    <%= f.label :date %><br />
    <%= f.date_select :date %>
  </div>
<div class="form-group col-md-6">
  <%= f.label :start_time %><br />
  <%= f.time_select :start_time %></br></br></br></br>
</div>
<div class="form-group col-md-6">
  <%= f.label :end_time %><br />
  <%= f.time_select :end_time %></br></br></br></br>
</div>
<div class="form-group col-md-6">
  <%= f.label :slot_count_constant %><br />
  <%= f.time_select :slot_count_constant %></br></br></br></br>
</div>
  <%= f.submit 'Create', :name => nil, :class => "btn btn-primary upbedbtn" %>
</p>
<% end %>
