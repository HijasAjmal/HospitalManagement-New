<div class="container border w-50" style="height: 20%;">
  <% form_for @photo, :url => photos_path, :html => {:multipart => true} do |f| %>
   <div class="text-center">
    <h1>Add Photo</h1>
  </div></br>
  <div class="form-row">
    <div class="form-group col-md-6">
      <%= f.error_messages %>
      <%= f.file_field :photo%>
    </div>
  </div></br>
  <%= f.submit 'Create', :class => "btn btn-primary btnphoto" %>
<% end %>
</div>
