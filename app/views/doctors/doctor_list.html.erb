<div class="bs-example">
  </br></br>
  <h2 class="text-left"><b>All Doctors</b></h2>
  <table class="table table-striped" style="margin-top: 1%;">
    <thead class="thead-dark">
      <tr>
        <th>Firstname</th>
        <th>Lastname</th>
        <th>Contact</th>
        <th>Email</th>
        <th>Department</th>
      </tr>
    </thead>
    <% @doctors.each do |doctor| %>
    <tr>
      <td><%=doctor.first_name %></td>
      <td><%=doctor.last_name %></td>
      <td><%=doctor.contact_number %></td>
      <td><%=doctor.email %></td>
      <td><%=doctor.department.department_name %></td>
    </tr>
    <% end %>
    </tbody>
  </table>
</div>
